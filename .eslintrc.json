{
  "env": {
    "node": true,
    "jest": true
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "no-console": "warn",
    "consistent-return": "warn",
    "max-classes-per-file": "off",
    "no-bitwise": "warn",
    "no-restricted-syntax": "warn",
    "no-continue": "warn"
  }
}
